{
    "plugins": [
        "plugins/markdown", 
        "jsdoc-plugin-typescript"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.(js|jsx|ts|tsx)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "typescript": {
        "moduleRoot": "src"
    }
}